{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng-github.com-fullstack-lang-gong-lib-markdown"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng-github.com-fullstack-lang-gong-lib-markdown"
        },
        {
            "name": "go server -unmarshallFromCode=data/stage.go -marshallOnCommit=data/stage",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/markdown",
            "args": [
                "-unmarshallFromCode=data/stage.go",
                "-marshallOnCommit=data/stage"
            ]
        },
    ]
}
